{
  "groups": [
    "mrhjkim",
    "wheel",
    "sysadmin",
    "dialout",
    "cdrom",
    "floppy",
    "audio",
    "video",
    "plugdev",
    "sambashare",
    "sbuild"
  ],
  "comment": "Kim Hae-Joo",
  "ssh_keys": "ssh-rsa  mrhjkim@mrhjkim.local",
  "id": "mrhjkim",
  "uid": 501,
  "shell": "\/bin\/bash",
  "repos": {
    /*
      Repositories that should be checked out. They have the following
      structure:
      "path_under_$HOME": {
        "repo": "git_url_to_repository",
        "revision": "HEAD"
      }
    */
    // Examples:
    /*
    ".rbenv": {
      "repo": "git://github.com/sstephenson/rbenv.git",
      "revision": "HEAD"
    }
	*/
	/*
    "Development/uangel/chef": {
      "repo": "git@github.com:mrhjkim/chef.git", // your fork of Chef
      "revision": "HEAD"
    }
	*/
  },
  "files": {
    /*
      Files that live in users cookbook for the user. For example,
      dotfiles. Note that in order for Chef to find dot files, they
      can't start with a dot character. They have the following
      structure:
      ".file_under_$HOME": {
        "mode": "0644",
        "source": "dot_file"
      }
     */
  }
}
